{
    "name": "refactor-annotation",
    "displayName": "refactor-annotation",
    "description": "with this extension you will be able to annotate all the tech debt or part to refactor of your Javascript application",
    "version": "0.0.1",
    "publisher": "lucamezzaliraa",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.getRefactorAnnotation"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.getRefactorAnnotation",
            "title": "Refactor Annotation"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}